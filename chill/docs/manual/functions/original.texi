void mm(float **A, float **B, float **C,
        int ambn, int an, int bm) {
  int i, j, n;
  for (i = 0; i <= an - 1; i += 1) 
    for (j = 0; j <= bm - 1; j += 1) {
      C[i][j] = 0.0f;
      C[i][j] += (A[i][0] * B[0][j]);
      for (n = 1; n <= ambn - 1; n += 1) 
        C[i][j] += (A[i][n] * B[n][j]);
    }
}
@end verbatim
