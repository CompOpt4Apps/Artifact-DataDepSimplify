# From Fabien Coelho

R := { [i, j, k, l, m, n, o, p] :
 0 <= i <= 3 && 
 0 <= j <= 3 &&
 0 <= k <= 3 &&
 0 <= l <= 3 &&
 0 <= m <= 3 &&
 0 <= n <= 3 &&
 1 <= o <= 1000 && o <= p <= 2 o +1 && 
 0 <= -1000 i + o + p <= 999 &&
 0 <= -1000 j - o + 2p <= 999 &&
 -1 <= -1000 k + 3 o <= 998 &&
 2 <= -1000 l + 2 o + p <= 1001 &&
 0 <= -1000 m + o <= 999 &&
 3 <= -1000 n + 2 p<= 1002
} ;

codegen R;
