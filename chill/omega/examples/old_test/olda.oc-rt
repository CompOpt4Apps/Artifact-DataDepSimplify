>>> codegen T10:IS10,T20:IS20;
if (np >= 1) {
  for(t1 = 1; t1 <= morb; t1++) {
    for(t2 = 1; t2 <= np; t2++) {
      for(t3 = 1; t3 <= t2-1; t3++) {
        s2(t2,t3,t1);
      }
      s1(t2,t2,t1);
      s2(t2,t2,t1);
      for(t3 = t2+1; t3 <= np; t3++) {
        s1(t3,t2,t1);
      }
    }
  }
}

