# From Fabien Coelho

R := { [i,j,k,l,m,n] :
  0 <= i <= 3  &&
  0 <= j <= 3  &&
  0 <= k <= 3  &&
  0 <= l <= 3  &&
  1 <= m <= 1000  &&
  -1 <= 3 m - 1000 k <= 998  &&
  m <= n <= 2 m + 1  &&
  0 <= m + n - 1000 i <= 999  &&
  0 <= 2 n - m - 1000 j <= 999  &&
  2 <= n + 2 m - 1000 l <= 1001
} ;

codegen R;
codegen R;
codegen R;
codegen R;
codegen R;
